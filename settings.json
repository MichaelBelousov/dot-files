    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "css.validate": false,
    "less.validate": false,
    "scss.validate": false,
    "editor.codeActionsOnSave": {
        "source.fixAll": true,
        "source.fixAll.stylelint": true,
        "source.fixAll.eslint": true
    },
    "editor.formatOnSave": true,
    "eslint.options": {
        "configFile": ".eslintrc.json"
    },
    "git.path": "C:\\msys64\\usr\\bin\\git.exe",
    "peacock.favoriteColors": [
        {
